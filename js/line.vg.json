{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 750,
    "height": 400,
    "description": "Line Chart",
    "mark": {"type": "line", "point": true},
    "data": {"url": "https://raw.githubusercontent.com/bjohn732/FIT3179_DataVis2/main/data/netflix_data_pt4.csv"},
      "encoding": {
        "x": {"field": "variable",
              "axis": {"labelAngle": -40, "labelFontSize": 11},
              "title": null},
        "y": {"aggregate":"mean",
              "field": "value",
              "type": "quantitative",
              "title": "Average Revenue ($)",
              "axis": {"labelFontSize": 10}},
        "color": {"field": "Region", 
                  "type": "nominal",
                  "scale": {
                      "range": ["#eb9172", "#cf5246", "#ab162a", "#67001f"]
                  }
                },
        "tooltip": [
          {"field": "variable", "type": "nominal", "title": "Quarter"},
          {"field": "value", "type": "quantitative", "title": "Netflix Revenue"}
        ]
      }
  }

  