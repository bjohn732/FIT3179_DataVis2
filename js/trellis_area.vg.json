{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 725,
  "height": 100,
  "description": "Netflix's Subscriber Figues, by Region (2018 to Q2 2021)",
  "data": {"url": "https://raw.githubusercontent.com/bjohn732/FIT3179_DataVis2/main/data/netflix_data_pt5.csv"},
  "mark": "area",
  "encoding": {
    "x": {
      "field": "variable",
      "axis": {"labelAngle": -40, "labelFontSize": 11},
      "type": "nominal",
      "title": null
    },
    "y": {
      "aggregate": "mean",
      "field": "value",
      "type": "quantitative",
      "title": null,
      "axis": {"labelFontSize": 10}
    },
    "color": {
      "field": "Region",
      "type": "nominal",
      "scale": {
        "range": ["#eb9172", "#cf5246", "#ab162a", "#67001f"]
    }
    },
    "row": {
      "field": "Region",
      "type": "nominal",
      "title": "Total Subscribers"

    },
    "tooltip": [
      {"field": "variable", "type": "nominal", "title": "Quarter"},
      {"field": "value", "type": "quantitative", "title": "Netflix Subscribers"}
    ]
  }
}